{
  "name": "interviewer_website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
      "start": "node index.js",
      "start:prod": "NODE_ENV=production node index.js",
      "start:prod:with-redis": "concurrently \"npm run redis\" \"npm run start:prod\"",
      "server": "nodemon index.js",
      "redis": "redis-server",
      "dev": "concurrently \"npm run redis\" \"npm run server\"",
      "client": "cd ../client && npm run dev",
      "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
      "build:ui": "npm i && rm -rf dist && cd ../client && npm install @vitejs/plugin-react --save && npm i && npm run build && cp -r dist ../server",
      "deploy:full": "npm run build:ui && git add . && git commit -m \"uibuild\" && git push",
      "deploy:prod": "npm run build:ui && git add . && git commit -m \"prod deploy\" && git push && npm run start:prod:with-redis"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
      "axios": "^1.8.4",
      "concurrently": "^9.1.2",
      "connect-redis": "^8.0.2",
      "cors": "^2.8.5",
      "date-fns": "^3.6.0",
      "dotenv": "^16.4.7",
      "express": "^4.21.2",
      "express-session": "^1.18.1",
      "fs": "^0.0.1-security",
      "http": "^0.0.1-security",
      "mic": "^2.1.2",
      "mongoose": "^8.13.2",
      "multer": "^1.4.5-lts.2",
      "openai": "^4.87.3",
      "os": "^0.1.2",
      "redis": "^4.7.0",
      "socket.io": "^4.8.1",
      "stream": "^0.0.3",
      "uuid": "^10.0.0"
  },
  "devDependencies": {
      "@types/node": "^22.5.0",
      "nodemon": "^3.1.4",
      "vite": "^6.2.5"
  }
}